e(prompt_prefix, \"\").replace(\"###>\", \"\").strip() for t in predictions]\n","\n","  # convert targets into format expected by BLEU score function\n","  targets = [[t] for t in targets]\n","\n","  # print(\"translations:\", predictions)\n","  # print(\"targets:\", targets)\n","\n","  # compare translations with targets\n","  scores = sacrebleu_metric.compute(predictions=predictions, references=targets)\n","  return scores\n"],"metadata":{"id":"MYeOQU1Afjc1","executionInfo":{"status":"ok","timestamp":1712194955334,"user_tz":240,"elapsed":312,"user":{"displayName":"Tanner Ducharme","userId":"07594315806145751874"}}},"execution_count":34,"outputs":[]},{"cell_type":"code","source":["from transformers import pipeline\n","\n","# create text-generation pipeline\n","generator = pipeline(\"text-generation\", model=model, tokenizer=tokenizer, max_new_tokens=64)\n","\n","# establish number of samples to generate\n","# set each to respective lengths if you want to translate every sentence in the test sets\n","\n","# num_supara_samples = len(supara_source_val)\n","# num_rising_samples = len(rising_source_val)\n","num_supara_samples = 3\n","num_rising_samples = 3\n","\n","\n","# get SUPara predictions\n","supara_predictions = generator(supara_dataset['source'][:num_supara_samples])\n","\n","# get RisingNews predictions\n","rising_predictions = generator(rising_dataset['source'][:num_rising_samples])\n","\n","# get SUPara BLEU scores\n","supara_scores_pre = get_translation_scores(\n","    generator,\n","    supara_predictions,\n","    supara_dataset['target'][:num_supara_samples],\n","    tokenizer\n","    )\n","print(f\"\\nSUPara BLEU score (pre-training): {supara_scores_pre}\")\n","\n","# get RisingNews BLEU scores\n","rising_scores_pre = get_translation_scores(\n","    generator,\n","    rising_predictions,\n","    rising_dataset['target'][:num_rising_samples],\n","    tokenizer\n","    )\n","print(f\"RisingNews BLEU score (pre-training): {rising_scores_pre}\")\n"],"metadata