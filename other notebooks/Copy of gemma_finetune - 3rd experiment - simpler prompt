","  decoded_labels = [clean_text(label) for label in decoded_labels]\n","\n","  # Some simple post-processing\n","  decoded_preds = [pred.strip() for pred in decoded_preds]\n","  decoded_labels = [[label.strip()] for label in decoded_labels]\n","\n","\n","  ######### DEBUGGING ############\n","  # num_print = 2\n","  # print(\"\\n\\n\")\n","  # for p,l in zip(decoded_preds[:num_print], decoded_labels[:num_print]):\n","  #   print(f\"pred: {p}\")\n","  #   print(f\"label: {l[0]}\\n\\n\")\n","\n","  ##################################\n","\n","\n","\n","\n","\n","  sacrebleu_result = sacrebleu_metric.compute(predictions=decoded_preds, references=decoded_labels)\n","\n","  # Prepare result dictionary\n","  results = {\n","      # \"bleu\": bleu_result['score'],\n","      \"sacrebleu\": sacrebleu_result['score']\n","  }\n","\n","  return results\n","\n"],"metadata":{"id":"8_p6a61SladY","colab":{"base_uri":"https://localhost:8080/","height":240,"referenced_widgets":["a01e63a62ed844d5ac20677d5f10ea83","7fce74195ae9487fb0352a54e55bd8a8","9c2e1192d8ec484da66ba20c494c43f8","d5e2e282f0554a88b87c3e73baedff39","5cf5fceef5a840a19280f62efcd448fe","0960dd0493204284865cf6ee7f90330d","225892e4318c41f2b3797de9d5aedd3b","8d72c9047b1c4321998bcbd6f6f7c594","29673bcb64f043fdbda5f5edbe8f86d3","318085229b8645baa4627abec17f5a6b","17618b8f906e490ea231dad6a1d7f948","b063138c071947a7ad4f22c1563f01a0","427f2042f7614cd0b62fca8d0d80a135","adfa5fc3a3f0430e84f1fcf8b5a9d39e","171f7488b48d4588bec1293399086462","336dfb0c0db048cf8c8df2dbb47f60a0","4349ed2e9b4345d2b5fc1b2a62308eaa","5473a504686d42678c937162d8953baf","8959bfb7a10a4d979a1960da84e8c6f3","f396e8e9209f438daf307f2c6be25cc1","4acfa6bad0004ca0a4ff8bbc3f35a0ad","b9761df3ee42455bb441b7a233497fdf"]},"executionInfo":{"status":"ok","timestamp":1712194770517,"user_tz":240,"elapsed":3424,"user":{"displayName":"Tanner Ducharme","userId":"07594315806145751874"}},"outputId":"b255e71b-793a-4753-98f6-8fc444d108fe"},"execution_count":27,"outputs":[{"output_