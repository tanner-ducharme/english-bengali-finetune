ns to remove '_eval' suffix\n","    cleaned_columns = {col: col.replace('_eval', '') for col in merged_data.columns}\n","    renamed_data = merged_data.rename(columns=cleaned_columns)\n","\n","    return renamed_data\n","\n","# Example usage with the simulated 'log_history' data\n","log_history = trainer.state.log_history\n","df = format_log_history(log_history)\n","df.to_csv(f\"results/{hub_model_id}/log_history.csv\", index=False)\n","df"],"metadata":{"id":"gkxim-3H1KAl","colab":{"base_uri":"https://localhost:8080/","height":571},"executionInfo":{"status":"ok","timestamp":1712196747922,"user_tz":240,"elapsed":620,"user":{"displayName":"Tanner Ducharme","userId":"07594315806145751874"}},"outputId":"7fe59f14-46e0-4517-e16b-73bc627310f7"},"execution_count":42,"outputs":[{"output_type":"execute_result","data":{"text/plain":["    loss_train  grad_norm_train  learning_rate_train  epoch_train  step  \\\n","0       2.5650         1.104686         6.666667e-07         0.00     1   \n","1       2.3397         1.385794         6.666667e-05         0.01   100   \n","2       2.1547         2.021014         9.629630e-05         0.02   200   \n","3       2.0886         1.533528         8.888889e-05         0.02   300   \n","4       2.0398         1.828677         8.148148e-05         0.03   400   \n","5       2.0152         1.768481         7.407407e-05         0.04   500   \n","6       2.0185         1.335019         6.666667e-05         0.05   600   \n","7       2.0139         3.216084         5.925926e-05         0.06   700   \n","8       2.0117         1.495562         5.185185e-05         0.06   800   \n","9       2.0077         1.662673         4.444444e-05         0.07   900   \n","10      2.0156         2.029929         3.703704e-05         0.08  1000   \n","11      1.9728         1.230241         2.962963e-05         0.09  1100   \n","12      2.0132         1.509326         2.222222e-05         0.10  1200   \n","13      1.9656         1.877164         1.481481e-05      