\n","    except FileNotFoundError:\n","        df_existing = pd.DataFrame(columns=['source', 'target', 'prediction'])\n","\n","    # Create new dataframe from current predictions\n","    df_new = pd.DataFrame({\n","        'source': supara_dataset['source'][index-100:index],\n","        'target': supara_dataset['target'][index-100:index],\n","        'prediction': predictions[-100:]\n","    })\n","\n","    # Append new data and save\n","    df_combined = pd.concat([df_existing, df_new], ignore_index=True)\n","    df_combined.to_csv(f\"results/{exp_name}/gemma_7b_supara_preds.csv\", index=False)\n","\n","# Adjust for your needs\n","num_predictions = len(supara_dataset['source'])\n","\n","torch.cuda.empty_cache()\n","for i, source in enumerate(tqdm(supara_dataset['source'][:num_predictions])):\n","    torch.cuda.empty_cache()\n","    tokenized_input = tokenizer(source, return_tensors=\"pt\")\n","    input_ids = tokenized_input[\"input_ids\"].cuda()\n","    del tokenized_input\n","    generation_output = full_model.generate(\n","        input_ids=input_ids,\n","        num_beams=6,\n","        return_dict_in_generate=True,\n","        output_scores=True,\n","        max_new_tokens=130\n","    )\n","    del input_ids\n","    for seq in generation_output.sequences:\n","        output = tokenizer.decode(seq, skip_special_tokens=True)\n","        pattern = r'Translate the following Bengali text into English:.*?\\nmodel\\n(.*)'\n","        match = re.search(pattern, output, re.DOTALL)  # re.DOTALL allows '.' to match newlines as well\n","\n","        if match:\n","            prediction = match.group(1).strip()  # .strip() to remove any leading or trailing whitespace\n","            print(\"Extracted text:\", prediction)\n","        else:\n","            print(\"No match found.\")\n","\n","\n","        predictions.append(prediction)\n","\n","    # Save every 100 predictions\n","    if (i + 1) % 100 == 0:\n","        save_dataframe(i + 1)\n","        print(f\"\\nSOURCE:\\n{source}\")\n","        print(f\"PREDICTION:\\n{prediction}\")\n","        print(f\"TARGET:\\n{supara_dataset['target'][i]}\\n\\n\")\n","\n","torch.cuda.empty_cache()\n","\n","# Save remaining predictions if there are any\n","if len(predictions) % 100 > 0:\n","    save_dataframe(len(predictions))\n"],"metadata":{"colab":{"base_uri":"https://localhost:8080/"},"id":"q5_-kl7-RtnH","outputId":"1d12ea4a-73a3-4661-da4a-a9e53a379112","executionInfo":{"status":"ok","timestamp":1713994629895,"user_tz":240,"elapsed":2253486,"user":{"displayName":"Tanner Ducharme","userId":"07594315806145751874"}}},"execution_count":39,"outputs":[{"output_type":"stream","name":"stderr","text":["  0%|          | 1/500 [00:01<11:30,  1.38s/it]"]},{"output_type":"stream","name":"stdout","text":["Extracted text: I love my homeland.\n"]},{"output_type":"stream","name":"stderr","text":["\r  0%|          | 2/500 [00:02<11:25,  1.38s/it]"]},{"output_type":"stream","name":"stdout","text":["Extracted text: We live in society.\n"]},{"output_type":"stream","name":"stderr","text":["\r  1%|          | 3/500 [00:04<12:15,  1.48s/it]"]},{"output_type":"stream","name":"stdout","text":["Extracted text: Ignorance is the companion of darkness.\n"]},{"output_type":"stream","name":"stderr","text":["\r  1%|          | 4/500 [00:06<13:30,  1.63s/it]"]},{"output_type":"stream","name":"stdout","text":["Extracted text: The cataclysm was caused by an earthquake.\n"]},{"output_type":"stream","name":"stderr","text":["\r  1%|          | 5/500 [00:07<13:48,  1.67s/it]"]},{"output_type":"stream","name":"stdout","text":["Extracted text: The book is man's best friend.\n"]},{"output_type":"stream","name":"stderr","text":["\r  1%|          | 6/500 [00:25<57:20,  6.96s/it]"]},{"output_type":"stream","name":"stdout","text":["Extracted text: English is an international language. ].(].(].(].(].(].(].(].(].(].(].(].(].(].(].(].(].(].(].(].(].(].(].(].(].(].(].(].(].(].(].(].(].(].(].(].(].(].(].(].(].(].(].(].(].(].(].(].(].(].(].(].(].