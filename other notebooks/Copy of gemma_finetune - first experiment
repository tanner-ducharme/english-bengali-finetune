ranslations = generator(inputs)\n","\n","  translations = [t[0]['generated_text'] for t in translations]\n","\n","  tokenized_translations = tokenizer(translations)\n","  tokenized_targets = tokenizer(targets)\n","\n","\n","  string_token_translations = [tokenizer.convert_ids_to_tokens(t) for t in tokenized_translations['input_ids']]\n","  string_token_targets = [tokenizer.convert_ids_to_tokens(t) for t in tokenized_targets['input_ids']]\n","\n","  print(string_token_translations)\n","  print(string_token_targets)\n","\n","  # compare translations with targets\n","  scores = sacrebleu_metric.compute(predictions=string_token_translations, references=string_token_targets)\n","  return scores\n","\n"],"metadata":{"id":"-ZX63nEeJnX7"},"execution_count":null,"outputs":[]},{"cell_type":"code","source":[],"metadata":{"id":"QhdQK3-uP2Ax"},"execution_count":null,"outputs":[]},{"cell_type":"code","source":["get_translation_scores(\n","    generator,\n","    source_supara_val[:2],\n","    target_supara_val[:2],\n","    tokenizer\n","    )"],"metadata":{"id":"s0t7sqXlOxtG"},"execution_count":null,"outputs":[]},{"cell_type":"markdown","source":[],"metadata":{"id":"iYNgYKFwpH1v"}},{"cell_type":"markdown","source":[],"metadata":{"id":"NY05D1g0qKsn"}},{"cell_type":"markdown","source":[],"metadata":{"id":"Bf8SmvdxpH31"}}]}