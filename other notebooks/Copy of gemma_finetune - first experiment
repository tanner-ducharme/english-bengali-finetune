ed_columns = {col: col.replace('_eval', '') for col in merged_data.columns}\n","    renamed_data = merged_data.rename(columns=cleaned_columns)\n","\n","    return renamed_data\n","\n","# Example usage with the simulated 'log_history' data\n","log_history = trainer.state.log_history\n","df = format_log_history(log_history)\n","df.to_csv(f\"{hub_model_id}/log_history.csv\", index=False)\n","df"],"metadata":{"colab":{"base_uri":"https://localhost:8080/","height":444},"id":"gkxim-3H1KAl","executionInfo":{"status":"ok","timestamp":1711752607177,"user_tz":240,"elapsed":254,"user":{"displayName":"Tanner Ducharme","userId":"07594315806145751874"}},"outputId":"9d7fb029-3f23-4742-cd17-2a076e1df334"},"execution_count":null,"outputs":[{"output_type":"execute_result","data":{"text/plain":["     loss_train  grad_norm_train  learning_rate_train  epoch_train  step  \\\n","0        3.1558         0.950366         2.000000e-08         0.00     1   \n","1        2.8614         0.880849         2.000000e-07         0.00    10   \n","2        2.9328         1.038457         4.000000e-07         0.00    20   \n","3        2.6646         0.797988         6.000000e-07         0.00    30   \n","4        2.8442         0.939131         8.000000e-07         0.00    40   \n","..          ...              ...                  ...          ...   ...   \n","146      1.3108         1.735118         2.920000e-05         0.03  1460   \n","147      1.0297         1.801670         2.940000e-05         0.03  1470   \n","148      1.2082         1.786912         2.960000e-05         0.03  1480   \n","149      1.2714         1.774126         2.980000e-05         0.03  1490   \n","150      1.2690         1.889982         3.000000e-05         0.03  1500   \n","\n","     epoch  eval_loss  eval_sacrebleu  eval_runtime  eval_samples_per_second  \\\n","0      NaN        NaN             NaN           NaN                      NaN   \n","1     0.00   2.453062       10.422872       20.1718                   29.596 